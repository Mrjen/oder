/* pages/orderReserve/orderReserve.wxss */
page{
  background: #f3f3f3;
}

.top_nav{
  width: 100%;
  height: 116rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.top_nav navigator{
  display: block;
  width: 232rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 26rpx;
  box-sizing: border-box;
}

.top_nav navigator:nth-of-type(1){
  background: #fff;
  color: #444;
  border: 1px solid #444;
}

.top_nav navigator:nth-of-type(2){
  background: #444;
  color: #fff;
}


.logo{
  width: 350rpx;
  height: 350rpx;
  margin:200rpx auto 100rpx auto;
  display: block;
}

.order_btn{
  width: 350rpx;
  height: 80rpx;
  background: #444;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  margin:20rpx auto;
}
